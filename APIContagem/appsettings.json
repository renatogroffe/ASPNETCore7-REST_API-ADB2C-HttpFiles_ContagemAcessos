{
  "Saudacao": "Testes com ASP.NET Core + JWT + Azure AD B2C + Swagger/OpenApi",
  "AzureAdB2C": {
    "Instance": "https://<tenant>.b2clogin.com",
    "ClientId": "<application_id>",
    "Domain": "<tenant>.onmicrosoft.com",
    "SignedOutCallbackPath": "/signout/<policy_name>",
    "SignUpSignInPolicyId": "<policy_name>"
  },
 "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}